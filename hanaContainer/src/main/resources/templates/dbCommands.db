create column table vendor(
	id varchar(80),
	firstname nvarchar(80),
	lastname nvarchar(60),
	companyname nvarchar(120),
	website nvarchar(65),
	email nvarchar(80),
	status nvarchar(10),
	gstNo nvarchar(32),
	primary key(id)
);


